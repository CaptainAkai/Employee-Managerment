<section class="content">
    <div class="container-fluid">
        <div class="col-12 m-3"></div>
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                            EMPLOYEE
                            {{!-- <small> Use <code>.table-striped</code> to add zebra-striping to any table row within the <code>&lt;tbody&gt;</code></small> --}}
                        </h2>
                        <ul class="header-dropdown m-r--5">
                            <button type="button" class="btn bg-deep-orange waves-effect" data-toggle="modal"
                                data-target="#addEmployee">ADD</button>
                        </ul>
                    </div>
                    <div class="body table-responsive">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Phone</th>
                                    <th>City</th>
                                    <th>Postion</th>
                                    <th> </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each employeeList}}
                                <tr>
                                    <td><img src="{{this.avatar}}" alt="" width="100"></td>
                                    <td>{{this.fullName}}</td>
                                    <td>{{this.email}}</td>
                                    <td>{{this.mobile}}</td>
                                    <td>{{this.city}}</td>
                                    <td>{{this.position.typeName}}</td>
                                    <td>
                                        <a type="button" data-toggle="modal" data-target="#editEmployee"
                                            data-id="{{this._id}}" data-fullName="{{this.fullName}}"
                                            data-email="{{this.email}}" data-mobile="{{this.mobile}}"
                                            data-position="{{this.position}}" data-city="{{this.city}}"
                                            data-avatar="{{this.avatar}}"
                                            class="btn btn-link bg-orange waves-effect">Edit</a>
                                        <a type="button" class="btn btn-link bg-blue-grey waves-effect"
                                            data-toggle="modal" data-target="#deleteEmployee"
                                            data-id="{{this._id}}">Delete</a>
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>


                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="editEmployee" tabindex="-1" role="dialog" style="display: none;">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="defaultModalLabel">EDIT EMPLOYEE</h4>
                </div>
                <form class="form-horizontal" action="/employee/update" method="POST">
                    <div class="modal-body">
                        <div class="col-lg-12">
                            <div class="grid">
                                <div class="grid-body">
                                    <div class="item-wrapper">
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="email_address_2">Id:</label>
                                            </div>
                                            <div class="col-lg-8 col-md-10 col-sm-8 col-xs-7">
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" id="id" class="form-control" name="id"
                                                            readonly>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="email_address_2">Avatar:</label>
                                            </div>
                                            <div class="col-lg-8 col-md-10 col-sm-8 col-xs-7">
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" id="avatar" class="form-control"
                                                            name="avatar" readonly>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="email_address_2">Name:</label>
                                            </div>
                                            <div class="col-lg-8 col-md-10 col-sm-8 col-xs-7">
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" class="form-control" id="fullName"
                                                            name="fullName">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="password_2">Email:</label>
                                            </div>
                                            <div class="col-lg-8 col-md-10 col-sm-8 col-xs-7">
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" class="form-control" id="email" name="email">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="password_2">Phone:</label>
                                            </div>
                                            <div class="col-lg-8 col-md-10 col-sm-8 col-xs-7">
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" class="form-control" id="mobile"
                                                            name="mobile">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="password_2">City:</label>
                                            </div>
                                            <div class="col-lg-8 col-md-10 col-sm-8 col-xs-7">
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" class="form-control" id="city" name="city">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-lg-4 col-md-2 col-sm-4 col-xs-5 form-control-label">
                                                <label for="password_2">Position:</label>
                                            </div>
                                            <div class="col-sm-6">
                                                <select class="selectpicker" name="position">
                                                    {{#each empTypeList}}
                                                    <option value="{{this._id}}">{{this.typeName}}</option>
                                                    {{/each}}
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-link bg-deep-orange waves-effect">SUBMIT</button>
                        <button type="button" class="btn btn-link waves-effect" data-dismiss="modal">CLOSE</button>
                    </div>
