<script>
    $(document).ready(function () {

        $('#editEmployee').on('show.bs.modal', (event) => {

            var td = $(event.relatedTarget);

            //Get data
            var id = td.data('id');
            var avatar = td.data('avatar');
            var fullName = td.data('fullname');
            var email = td.data('email');
            var mobile = td.data('mobile');
            var city = td.data('city');
            var position = td.data('position');
            //set action form for submit
            //Set data to feild
            $(this).find('#editEmployee #id').val(id);
            $(this).find('#editEmployee #avatar').val(avatar);
            $(this).find('#editEmployee #fullName').val(fullName);
            $(this).find('#editEmployee #email').val(email);
            $(this).find('#editEmployee #mobile').val(mobile);
            $(this).find('#editEmployee #city').val(city);
            $(this).find('#editEmployee #position').val(position);
        });

        $('#deleteEmployee').on('show.bs.modal', (event) => {

            var td = $(event.relatedTarget);

            //Get data
            var id = td.data('id');

            $(this).find('#deleteEmployee #id').val(id);
        });
    });
</script>